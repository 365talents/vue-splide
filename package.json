{
  "name": "@splidejs/vue-splide",
  "version": "0.6.12",
  "description": "The Splide component for Vue.",
  "author": "Naotoshi Fujita",
  "license": "MIT",
  "main": "dist/vue-splide.umd.js",
  "module": "dist/vue-splide.mjs",
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@9.4.0",
  "files": [
    "dist"
  ],
  "keywords": [
    "splide",
    "slider",
    "carousel",
    "slideshow",
    "lightweight",
    "touch",
    "responsive",
    "vue"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Splidejs/vue-splide.git"
  },
  "bugs": {
    "url": "https://github.com/Splidejs/vue-splide/issues"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vitejs/plugin-vue": "^5.1.2",
    "eslint": "^8.23.0",
    "eslint-plugin-vue": "^9.4.0",
    "fs-extra": "^10.1.0",
    "gh-pages": "^4.0.0",
    "jest": "^29.0.2",
    "ts-jest": "next",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vite-plugin-dts": "4.0.0-beta.2",
    "vue": "^3.4.35",
    "vue-tsc": "^2.0.29"
  },
  "scripts": {
    "develop": "vite build",
    "check:type": "vue-tsc --noEmit",
    "build": "vite build",
    "build:all": "npm run build && node scripts/copy-css.js",
    "test": "jest",
    "eslint": "eslint src",
    "deploy": "gh-pages -d examples/dist",
    "prepare": "npm run build:all"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/vue-splide.umd.js",
      "import": "./dist/vue-splide.mjs",
      "default": "./dist/vue-splide.mjs"
    },
    "./css": "./dist/css/splide.min.css",
    "./css/core": "./dist/css/splide-core.min.css",
    "./css/*": "./dist/css/themes/splide-*.min.css"
  },
  "dependencies": {
    "@splidejs/splide": "^4.1.3"
  }
}
